{
  "name": "firebird-restapi",
  "version": "1.0.0",
  "description": "Firebird REST API server with CRUD functionality for PERSON table",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "tunnel": "ngrok http --url=prompt-liberal-vulture.ngrok-free.app 3000",
    "dev:all": "concurrently \"npm run dev\" \"npm run tunnel\""
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "iconv-lite": "^0.6.3",
    "joi": "^18.0.2",
    "ngrok": "^5.0.0-beta.2",
    "node-firebird": "^1.1.9",
    "node-windows": "^1.0.0-beta.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/helmet": "^0.0.48",
    "@types/iconv-lite": "^0.0.1",
    "@types/node": "^25.2.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
